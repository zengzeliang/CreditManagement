<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.credit.creditmanagement.mapper.AmountCategoryMapper">
    <select id="selectAmountCategoryByType" resultType="com.credit.creditmanagement.entity.AmountCategory">
        select id, amountType, amount from amount_category where amountType = #{amountType}
    </select>

</mapper>